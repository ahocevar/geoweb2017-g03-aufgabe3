{
  "version": 8,
  "name": "GeoWeb Demo",
  "metadata": {},
  "center": [
    16.3792,
    48.2231
  ],
  "zoom": 12,
  "sources": {
    "zaehlpunkte": {
      "type": "geojson",
      "data": "https://student.ifip.tuwien.ac.at/geoweb/2017/g03/map/postgis_geojson.php"
    },
    "bezirksgrenzenogd": {
      "type": "geojson",
      "data": "https://data.wien.gv.at/daten/geo?service=WFS&request=GetFeature&version=1.1.0&typeName=ogdwien:BEZIRKSGRENZEOGD&srsName=EPSG:4326&outputFormat=json"
    },
    "radwegeogd": {
      "type": "geojson",
      "data": "https://data.wien.gv.at/daten/geo?service=WFS&request=GetFeature&version=1.1.0&typeName=ogdwien:RADWEGEOGD&srsName=EPSG:4326&outputFormat=json"
    },
    "basemap": {
      "type": "raster",
      "tiles": [
        "https://maps.wien.gv.at/basemap/geolandbasemap/normal/google3857/{z}/{y}/{x}.png"
      ],
      "minzoom": 0,
      "maxzoom": 20,
      "tileSize": 256
    }
  },
  "glyphs": "https://demo.tileserver.org/fonts/{fontstack}/{range}.pbf",
  "layers": [{
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "rgba(255, 255, 255, 1)"
      }
    },
    {
      "id": "basemap",
      "type": "raster",
      "source": "basemap"
    },
    {
      "id": "ausserguertel",
      "type": "fill",
      "source": "bezirksgrenzenogd",
      "filter": [
        "all", [
          ">=",
          "DISTRICT_CODE",
          1110
        ]
      ],
      "paint": {
        "fill-color": "rgba(192, 192, 192, 1)",
        "fill-outline-color": "rgba(4, 4, 4, 1)",
        "fill-opacity": 0.9

      }
    },
    {
      "id": "innerguertel",
      "type": "fill",
      "source": "bezirksgrenzenogd",
      "filter": [
        "all", [
          ">",
          "DISTRICT_CODE",
          1010
        ],
        [
          "<",
          "DISTRICT_CODE",
          1110
        ]
      ],
      "paint": {
        "fill-color": "rgba(192, 192, 192, 1)",
        "fill-outline-color": "rgba(4, 4, 4, 1)",
        "fill-opacity": 0.9
      }
    },
    {
      "id": "innenstadt",
      "type": "fill",
      "source": "bezirksgrenzenogd",
      "minzoom": 0,
      "filter": [
        "all", [
          "==",
          "DISTRICT_CODE",
          1010
        ]
      ],
      "paint": {
        "fill-color": "rgba(192, 192, 192, 1)",
        "fill-outline-color": "rgba(4, 4, 4, 1)",
        "fill-opacity": 0.9
      }

    },
    {
      "id": "radwege",
      "type": "fill",
      "source": "radwegeogd",
      "minzoom": 0,
      "paint": {
        "fill-color": "rgba(192, 192, 192, 1)",
        "fill-outline-color": "rgba(4, 4, 4, 1)",
        "fill-opacity": 0.9
      }

    },
    {
      "id": "zaehlpunkte",
      "type": "circle",
      "source": "zaehlpunkte",
      "paint": {
        "circle-radius": 7,
        "circle-color": "rgba(0, 0, 0, 1)",
        "circle-stroke-color": "rgba(127, 127, 127, 1)",
        "circle-stroke-width": 1
      }
    }
  ],
  "sprites": "https://demo.tileserver.org/fonts/{fontstack}/{range}.pbf",
  "id": "kfp20g6n2"
}
